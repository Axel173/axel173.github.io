$(document).ready(function()
{
	$('.product_section_menu_more').click(function()
	{
		$(this).parent('.product_section_menu').toggleClass('open');
		if ($(this).children('span').html()=="+")
		{
			$(this).children('span').html('-');
		}
		else
		{
			$(this).children('span').html('+');
		}
		return false;
	});
});
/* End */
